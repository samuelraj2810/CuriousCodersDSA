#include <iostream>
using namespace std;

int main() {
    int mark;
    cin >> mark;
    if(mark>=90 && mark<=100) {
        cout <<"Grade A";
    } else if(mark>=80 && mark<90){
        cout << "Grade B";
    } else if(mark>=60 && mark<80){
        cout << "Grade C";
    }  else if(mark>=35 && mark<60){
        cout << "Grade D";
    }  else if(mark<35){
        cout << "Fail";
    }  else {
        cout<< "Enter a valid mark";
    }
    return 0;
}
