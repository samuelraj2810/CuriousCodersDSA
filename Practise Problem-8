#include <iostream>
using namespace std;

int addtwonumbers(int a, int b){
    int c = a+b;
    return c;
}

int main() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    int e = addtwonumbers(a,b);
    cout << "a+b : " << e << endl;
    int f = addtwonumbers(c,d);
    cout << "c+d : " << f << endl;
    int g = addtwonumbers(e,f);
    cout << "e+f : " << g << endl;
    
}
